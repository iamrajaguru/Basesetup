    "@types/history": "^4.7.5",
    "@types/invariant": "^2.2.31",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.4",
    "@types/redux": "^3.6.0",
    "@types/webpack-env": "^1.15.1",
    "autoprefixer": "^9.7.6",
    "axios": "^0.19.2",
    "clean-css-cli": "^4.3.0",
    "connected-react-router": "^6.8.0",
    "history": "^4.10.1",
    "invariant": "^2.2.4",
    "node-sass": "^4.13.1",
    "node-sass-tilde-importer": "^1.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "query-string": "^6.12.1",
